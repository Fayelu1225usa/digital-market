          <AuxValues>
            <AuxValue name="JavaCodeGenerator_InitCodePre" type="java.lang.String" value="DefaultTableModel dtm = new DefaultTableModel(0, 0);&#xa;&#xa;String[] header = new String[]{&quot;Customer&quot;, &quot;Market&quot;, &quot;Channel&quot;, &quot;Total Price&quot;};&#xa;dtm.setColumnIdentifiers(header);&#xa;&#xa;for(Order order : this.pending_orders){&#xa;    String[] row = new String[4];&#xa;    row[0] = order.getCustomer().getName();&#xa;    row[1] = String.join(&quot;,&quot;, order.getBundleAssignment().getMarketAssignMent().getMarket().getCharacters());&#xa;    row[2] = order.getBundleAssignment().getMarketAssignMent().getChannel().getType();&#xa;    row[3] = String.valueOf(order.getOrderTotal()); &#xa;    dtm.addRow(row);&#xa;}"/>
          </AuxValues>
        </Component>
      </SubComponents>
